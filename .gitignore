# Ignore node_modules (Frontend and Backend)
node_modules/

# Ignore build and distribution directories (Frontend and Backend)
build/
dist/

# Ignore log files and temporary files
*.log
*.tmp
*.swp
*.swo

# Ignore environment files (Frontend and Backend)
.env
.env.local
.env.*.local

# Ignore package-lock.json or yarn.lock (Frontend and Backend)
package-lock.json
yarn.lock

# Ignore dependency directories (Frontend and Backend)
/frontend/node_modules/
/backend/node_modules/

# Ignore logs and npm debug files
npm-debug.log
yarn-debug.log
yarn-error.log

# Ignore any .DS_Store files (macOS)
.DS_Store

# Ignore coverage folder (Frontend and Backend testing)
coverage/

# Ignore IDE files (VS Code, JetBrains, etc.)
.vscode/
.idea/

# Ignore frontend build outputs (if using React, Angular, etc.)
/frontend/build/
/frontend/dist/

# Ignore backend build outputs (if using any backend build tools like TypeScript)
/backend/build/
/backend/dist/

# Ignore other config or system files
*.bak
*.orig
*.swp
